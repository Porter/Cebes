{
  "name": "cebes",
  "version": "1.0.0",
  "description": "Like Socrates.io, but different",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha ./test/*/ --grep integration --invert",
    "test-int": "NODE_ENV=test mocha ./test/*/ --grep integration --timeout 10000",
    "test-all": "npm run test && npm run test-int",
    "start": "npm run start-dev",
    "start-dev": "NODE_ENV=development nodemon index.js",
    "start-test": "NODE_ENV=test PORT=9000 nodemon index.js",
    "migrate": "node migrations/migration_manager.js",
    "start-db": "sudo -u postgres postgres -D /usr/local/pgsql/data/"
  },
  "author": "Porter Haet",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "lodash": "^4.15.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.0.3",
    "request": "^2.74.0",
    "require-optional": "^0.3.0",
    "sequelize": "^3.24.3",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "chai-string": "^1.2.0",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0",
    "phantomjs": "^2.1.7",
    "sinon": "^1.17.5",
    "zombie": "^4.2.1"
  }
}
